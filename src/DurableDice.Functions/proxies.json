{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "index": {
      "matchCondition": {
        "methods": [ "GET", "OPTIONS" ],
        "route": "/"
      },
      "backendUri": "https://durabledice.blob.core.windows.net/",
      "debug": true
    },
    "gameid": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/{gameid}"
      },
      "backendUri": "https://durabledice.blob.core.windows.net/{gameid}",
      "debug": true
    },
    "content": {
      "matchCondition": {
        "methods": [ "GET", "OPTIONS" ],
        "route": "/_content/{*asset}"
      },
      "backendUri": "https://durabledice.blob.core.windows.net/site/_content/{asset}",
      "debug": true
    },
    "framework": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/_framework/{*asset}"
      },
      "backendUri": "https://durabledice.blob.core.windows.net/site/_framework/{asset}",
      "debug": true
    },
    "favicon": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/favicon.ico"
      },
      "backendUri": "https://durabledice.blob.core.windows.net/site/favicon.ico",
      "debug": true
    }
  }
}